<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="is.RealEstate.realEstateMate">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="is.RealEstate.realEstateMate.ui.PropertyActivity"></activity>
        <activity android:name="is.RealEstate.realEstateMate.ui.SignUpActivity"></activity>
        <activity android:name="is.RealEstate.realEstateMate.ui.AboutActivity"></activity>
        <activity android:name="is.RealEstate.realEstateMate.ui.UserHomeActivity"></activity>
        <activity
            android:name="is.RealEstate.realEstateMate.ui.LoginActivity"
            android:label="@string/title_activity_login" />
        <activity android:name="is.RealEstate.realEstateMate.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>